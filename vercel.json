{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.tdstudiosdigital.com"
        }
      ],
      "destination": "https://tdstudiosdigital.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "installCommand": "pnpm install --no-frozen-lockfile"
}
